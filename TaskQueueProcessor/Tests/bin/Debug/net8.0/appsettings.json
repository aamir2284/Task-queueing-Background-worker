{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=TaskQueueDemo;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Worker": {
    "MaxDegreeOfParallelism": 4,
    "RetryCount": 3,
    "RetryInitialDelayMs": 500
  },
  "Poller": {
    "PollingIntervalSeconds": 5,
    "BatchSize": 50
  },
  "Warmup": {
    "Enabled": true,
    "IntervalSeconds": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}
